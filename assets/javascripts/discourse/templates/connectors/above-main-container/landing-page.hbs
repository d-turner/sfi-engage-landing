{{#if showLandingPage}}
<div class="landingMain">
  <div class="multimedia">
    <img class="landingMedia" src="https://thumbs.dreamstime.com/z/tv-test-image-card-rainbow-multi-color-bars-geometric-signals-retro-hardware-s-minimal-pop-art-print-suitable-89603635.jpg" alt="Holding image" />
  </div>
  <div class="clockMain">
    <div class="clockWrapper" id="clockdiv">
      <span class="clockText">Some Time Should be updating here</span>
    </div>
  </div>

  <div class="eventsWrapper">
    {{#if showLandingPage}}
      {{event-list events=liveEvents label="Now On"}}
    {{/if}}
